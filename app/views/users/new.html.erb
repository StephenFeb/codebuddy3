<p>New User Page</p>
<div  class="container">
  <div class="row">
    <div class="col-xs-4">
<%= simple_form_for @user do |form| %>
    <%= form.input :name, label: "Name" %>
    <%= form.input :location, label: "Location" %>
    <%= form.input :languages, label: "Languages" %>
    <%= link_to "Create Profile", "/users/:id", class: "btn btn-primary" %>
  <% end %>
  </div>
  </div>
</div>